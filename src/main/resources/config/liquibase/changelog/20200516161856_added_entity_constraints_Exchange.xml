<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Exchange.
    -->
    <changeSet id="20200516161856-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="exchange_id"
                                 baseTableName="exchange_rates"
                                 constraintName="fk_exchange_rates_exchange_id"
                                 referencedColumnNames="id"
                                 referencedTableName="exchange"/>

        <addForeignKeyConstraint baseColumnNames="rates_id"
                                 baseTableName="exchange_rates"
                                 constraintName="fk_exchange_rates_rates_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rates"/>
        
    </changeSet>
</databaseChangeLog>
